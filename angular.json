{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "slides": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/slides",
      "sourceRoot": "projects/slides/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/slides",
            "index": "projects/slides/src/index.html",
            "main": "projects/slides/src/main.ts",
            "polyfills": "projects/slides/src/polyfills.ts",
            "tsConfig": "projects/slides/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/slides/src/favicon.ico",
              "projects/slides/src/assets"
            ],
            "styles": [
              "projects/slides/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/slides/src"
              ]
            },
            "scripts": [],
            "extraWebpackConfig": "projects/slides/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/slides/src/environments/environment.ts",
                  "with": "projects/slides/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "extraWebpackConfig": "projects/slides/webpack.prod.config.js"
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "slides:build",
            "extraWebpackConfig": "projects/slides/webpack.config.js",
            "port": 3000
          },
          "configurations": {
            "production": {
              "browserTarget": "slides:build:production",
              "extraWebpackConfig": "projects/slides/webpack.prod.config.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "slides:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/slides/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/host",
      "sourceRoot": "projects/host/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/host",
            "index": "projects/host/src/index.html",
            "main": "projects/host/src/main.ts",
            "polyfills": "projects/host/src/polyfills.ts",
            "tsConfig": "projects/host/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/host/src/favicon.ico",
              "projects/host/src/assets"
            ],
            "styles": [
              "projects/host/src/styles.scss"
            ],
            "scripts": [],
            "extraWebpackConfig": "projects/host/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/host/src/environments/environment.ts",
                  "with": "projects/host/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "extraWebpackConfig": "projects/host/webpack.prod.config.js"
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "host:build",
            "extraWebpackConfig": "projects/host/webpack.config.js",
            "port": 5000
          },
          "configurations": {
            "production": {
              "browserTarget": "host:build:production",
              "extraWebpackConfig": "projects/host/webpack.prod.config.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "host:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/host/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "slides",
  "cli": {
    "packageManager": "yarn"
  }
}
